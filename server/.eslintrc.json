{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "root": true,
  "extends": ["airbnb-base"],

  "plugins": ["prettier"],

  "rules": {
    "quotes": "off",
    "arrow-parens": "off",
    "spaced-comment": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "no-plusplus": "off",
    "no-await-in-loop": "off",
    "no-unused-vars": "warn",
    "comma-dangle": "off"
  }
}
