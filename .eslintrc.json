{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "root": true,
  "extends": ["prettier"],
  "plugins": [],

  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-use-before-define": "off",
    "react/destructuring-assignment": "off",
    "quotes": "off",
    "arrow-parens": "off",
    "spaced-comment": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "no-plusplus": "off",
    "no-await-in-loop": "off",
    "no-unused-vars": "warn",
    "comma-dangle": "off"
  }
}
